{
    "version": 2,
    "builds": [
        {
            "src": "frontend/starkbnb-web/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "framework": "nextjs",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/starkbnb-web"
        }
    ],
    "scripts": {
        "build": "npm install --legacy-peer-deps && npm run build"
    }
}